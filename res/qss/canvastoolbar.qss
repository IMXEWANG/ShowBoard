QPushButton,.QLabel {
    color:white;
    background-color:#00000000;
    border:none;
    font-size:14pt; /* [size] */
    font-family:'Microsoft YaHei';
}

QPushButton {
    qproperty-iconSize:32px 32px; /* [size] */
    padding:6px; /* [size] */
    border-radius:8px; /* [size] */
}

QPushButton:checked {
    background-color:#008FFF;
}

QPushButton:pressed {
    background-color:#1C61B0;
}

QPushButton:unchecked:hover {
    background-color:#464A4E;
}

#splitter {
    height: 32px; /* [size] */
    max-height: 32px; /* [size] */
    max-width: 1px;
    border: none;
    background-color: #333B3B;
}

#dragger {
    max-width: 22px; /* [size] */
    max-height: 32px; /* [size] */
    width: 22px; /* [size] */
    height: 32px; /* [size] */
    border: none;
    qproperty-pixmap: url(":/showboard/icon/drag.svg");
}

#canvastoolbar {
    background-color:#2B3034;
    border-radius:6px; /* [size] */
}

